@model Food.ViewModels.HomeIndexViewModel
@{
    ViewData["Title"] = "Food App";
}
<h3>All Restaurants</h3>

@* Partial Views are dependent views almost like html components *@
@foreach (var resto in Model.Restaurants)
{
    @Html.Partial("_Summary", resto);
}

<div>
    <a asp-action="Create">Add Restaurant</a>
</div>
